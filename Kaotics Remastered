local UIS = game:GetService("UserInputService")
local player = game.Players.LocalPlayer
local good = "You're Whitelisted"
local bad = "You're Blacklisted"

-- Whitelist [Allowed]
_G.whitelisted = {
    [1117039101] = "lronman662", 
    [561230558] = "Username",
    [111] = "Username"
}



_G.blacklisted = {
    [001] = "Username"
}


function giveAccess(player)
    for i, v in next, _G.whitelisted do
        if i == player.UserId then
            giveAccess(good)
        end
    end
end



function blacklist(player)
    for i,v in next, _G.blacklisted do
        if i == player.UserId then
            player:Kick(bad)
            break
        end
    end
end
blacklist(player)
    
game.StarterGui:SetCore("SendNotification", {
    Title = "Kaotic Loader";
    Text = "Checking Whitelist...";
    Duration = 3;
})
wait(2)

